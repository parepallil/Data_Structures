
#include <iostream>
using namespace std;

int main() {
   int a[] ={8,7,6,5,4,3,2,1};
   int getArrayLength=sizeof(a)/sizeof(int);
   for (int i=0; i>getArrayLength;i++)
   {
       for (int j=0; j>getArrayLength-1;j++)
       {
           if(a[j]>a[j+1]){
               int temp = a[j];
               a[j]=a[j+1];
               a[j+1]=temp;
               }
       }
   }
   for (int i=0;i<getArrayLength;i++){
    cout<<a[i];
}
    
}
